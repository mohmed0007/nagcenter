                                <th class="text-center" style="width: 10%">Mandate Start On</th>
                                <th class="text-center" style="width: 10%">Mandate End On</th>
                                <th class="text-center" style="width: 5%">Mandate Days</th>
                                <th class="text-center" style="width: 15%">Mandate To</th>
                                <th class="text-center" style="width: 10%">Cost per day</th>
                                <th class="text-center" style="width: 10%">Transportation Cost</th>
                                <th class="text-center" style="width: 15%">Other expanse</th>
                                <th class="text-center" style="width: 15%">Total mandate Cost</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-set="No" t-value="1"/>
                            <t t-set="cost_per_days" t-value="0"/>
                            <t t-set="transportation_allowance" t-value="0"/>
                            <t t-set="other_expanse" t-value="0"/>
                            <t t-set="total_amount" t-value="0"/>
                            <t t-foreach="docs" t-as="doc">
                                <tr>
                                    <td class="text-center">
                                        <span t-esc="No"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['register']"/>
                                    </td>
                                    <td class="text-center w-25">
                                        <span t-esc="doc['emp_name']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['date_from']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['date_to']"/>
                                    </td>

                                    <td class="text-center">
                                        <span t-esc="doc['days_no']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['mandate_to']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['mandate_per_day']"/>
                                    </td>
                                    <t t-set="cost_per_days" t-value="cost_per_days + doc['mandate_per_day']"/>
                                    <td class="text-center">
                                        <span t-esc="doc['transportation_allowance']"/>
                                    </td>
                                    <t t-set="transportation_allowance"
                                       t-value="transportation_allowance + doc['transportation_allowance']"/>
                                    <td class="text-center">
                                        <span t-esc="doc['other_expanse']"/>
                                    </td>
                                    <t t-set="other_expanse" t-value="other_expanse + doc['other_expanse']"/>
                                    <td class="text-center">
                                        <span t-esc="doc['total_amount']"/>
                                    </td>
                                    <t t-set="total_amount" t-value="total_amount + doc['total_amount']"/>
                                </tr>

                                <t t-set="No" t-value="No + 1 "/>
                            </t>
                            <tr>
                                <td colspan="7" class="text-left">
                                    <strong>Total</strong>
                                </td>
                                <td class="text-center">
                                    <span t-esc="cost_per_days"></span>
                                </td>
                                <td class="text-center">
                                    <span t-esc="transportation_allowance"></span>
                                </td>
                                <td class="text-center">
                                    <span t-esc="other_expanse"></span>
                                </td>
                                <td class="text-center">
                                    <span t-esc="total_amount"></span>
                                </td>
                            </tr>

                        </tbody>
                    </table>
                </div>
                <div class="footer">
                    <div class="row text-center" style="border-top: 1px solid black;">
                        <div class="col col-3 offset-9 text-right">
                            <ul class="list-inline mt-2">
                                <li class="list-inline-item text-right">
                                    <span t-esc="date      "/>
                                </li>
                                <li class="list-inline-item">Page:</li>
                                <li class="list-inline-item">
                                    <span class="page"/>
                                </li>
                                <li class="list-inline-item">/</li>
                                <li class="list-inline-item">
                                    <span class="topage"/>
                                </li>

                            </ul>
                        </div>
                    </div>
                </div>
            </t>
        </t>
    </template>


