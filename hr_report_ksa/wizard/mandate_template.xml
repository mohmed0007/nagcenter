                                <th class="text-center" style="width: 10%">Resignation date</th>
                                <th class="text-center" style="width: 15%">Reason</th>
                                <th class="text-center" style="width: 5%">Note</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-set="No" t-value="1"/>

                            <t t-foreach="docs" t-as="doc">
                                <tr>
                                    <td class="text-center">
                                        <span t-esc="No"/>
                                    </td>

                                    <td class="text-center">
                                        <span t-esc="doc['register']"/>
                                    </td>

                                    <td class="text-center">
                                        <span t-esc="doc['emp_name']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['department']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['education']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['first_contract_date']"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="doc['resignation_date']"/>
                                    </td>

                                    <td class="text-center">
                                        <span t-esc="doc['reason']"/>
                                    </td>
                                    <td class="text-center">

                                    </td>


                                </tr>

                                <t t-set="No" t-value="No + 1 "/>
                            </t>


                        </tbody>
                    </table>
                </div>

                <div>

                    <table class="table table-sm" style="width: 25% ! important;font-size:14px ! important;">
                        <tr style="text-align:center;">
                            <td style="text-align:left;color:#875A7B">
                                <strong>Total recruited</strong>
                            </td>
                            <td style="text-align:left;border:none;color:#875A7B">
                                <!--                                <span t-esc="arabian"/>-->
                            </td>
                            <!--                                <td style="text-align:left;color:#875A7B"><strong>Total recruited </strong></td>-->
                            <!--                                <td style="text-align:left;color:#875A7B" >-->
                            <!--&lt;!&ndash;                                <span t-esc="non_arabian"/>&ndash;&gt;-->
                            <!--                                </td>-->
                        </tr>

                        <tr>
                            <!--                             <tr style="text-align:center;">-->
                            <!--                                <td style="text-align:left;color:#875A7B"><strong>Total recruited</strong></td>-->
                            <!--                                <td style="text-align:left;border:none;color:#875A7B" >-->
                            <!--                                <span t-esc="arabian + non_arabian"/>-->
                            <!--                                </td>-->

                            <!--                             </tr>-->
                            <td style="text-align:left;color:#875A7B">
                                <strong>Total recruited</strong>
                            </td>
                            <td style="text-align:left;color:#875A7B">
                                <!--                                <span t-esc="non_arabian"/>-->
                            </td>
                        </tr>
                        <br/>
                        <br/>
                    </table>
                    <br/>
                    <br/>
                </div>
                <div class="footer">
                    <div class="row text-center" style="border-top: 1px solid black;">
                        <div class="col col-3 offset-9 text-right">
                            <ul class="list-inline mt-2">
                                <li class="list-inline-item text-right">
                                    <span t-esc="date      "/>
                                </li>
                                <li class="list-inline-item">Page:</li>
                                <li class="list-inline-item">
                                    <span class="page"/>
                                </li>
                                <li class="list-inline-item">/</li>
                                <li class="list-inline-item">
                                    <span class="topage"/>
                                </li>

                            </ul>
                        </div>
                    </div>
                </div>
            </t>
        </t>
    </template>


