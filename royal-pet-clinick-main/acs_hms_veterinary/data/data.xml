<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_acs_pet" model="ir.sequence">
            <field name="name">pet</field>
            <field name="code">acs.pet</field>
            <field name="prefix">PET</field>
            <field name="padding">3</field>
            <field name="acs_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="veterinary_department" model="hr.department">
            <field name="name">Veterinary</field>
            <field name="patient_department">True</field>
            <field name="department_type">veterinary</field>
        </record>

    </data>
</odoo>
