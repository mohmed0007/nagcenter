<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_flight_booking" model="ir.sequence">
            <field name="name">Flight Booking</field>
            <field name="code">flight.booking</field>
            <field name="prefix">FB/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

        <record id="sequence_business_trip" model="ir.sequence">
            <field name="name">Buisness Trip</field>
            <field name="code">business.trip</field>
            <field name="prefix">BT/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
