<?xml version="1.0"?>
<odoo>

    <record id="action_acs_veterinary_procedure" model="ir.actions.act_window">
        <field name="name">Patient Veterinary Procedures</field>
        <field name="res_model">acs.patient.procedure</field>
        <field name="view_mode">tree,form,calendar,pivot</field>
        <field name="context">{'search_default_not_done': 1}</field>
        <field name="domain" eval="[('department_id.department_type','=','veterinary')]"/>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Click to add a Patient Veterinary Procedure.
            </p>
        </field>
    </record>

</odoo>