<odoo>
    <data noupdate='1'>
		<record id="ir_seq_ticket_request" model="ir.sequence">
			<field name="name">Ticket Request</field>
			<field name="code">ticket.request.seq</field>
			<field name="prefix">TR/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
		<record id="ir_seq_leave_clearance" model="ir.sequence">
			<field name="name">Leave Clearance</field>
			<field name="code">leave.clearance</field>
			<field name="prefix">LCL/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>

	</data>
</odoo>