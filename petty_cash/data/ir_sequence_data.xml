<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences for petty cash -->
        <record id="seq_tran_number_financial_dependents" model="ir.sequence">
            <field name="name">Dependents No</field>
            <field name="code">financial.dependents</field>
            <field name="prefix">PettyCash/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for liquidation.dependents-->
        <record id="seq_tran_number_liquidation_dependents" model="ir.sequence">
            <field name="name">Liquidation No</field>
            <field name="code">liquidation.dependents</field>
            <field name="prefix">Petty Cash Clearance/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>