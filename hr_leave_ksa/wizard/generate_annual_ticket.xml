<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_annual_ticket_by_employees" model="ir.ui.view">
        <field name="name">annual_ticket_employees</field>
        <field name="model">annual.ticket.employees</field>
        <field name="arch" type="xml">
            <form string="Annual Ticket by Employees">
                <group colspan="4" >
                    <separator string="Employees" colspan="4"/>
                    <newline/>
                    <field name="employee_ids" nolabel="1"/>
                </group>
                <footer>
                    <button string="Add" name="generate_records" type="object" class="oe_highlight"/>
                    or
                    <button class="oe_link" string="Cancel" special="cancel"/>
                </footer>
           </form>
        </field>
    </record>

</odoo>
